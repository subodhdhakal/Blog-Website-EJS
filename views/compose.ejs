<%- include("partials/header"); -%>
<h1>Compose</h1>
<form action="/compose" method="POST">
    <div class="form-group">
        <label>Title</label>
        <textarea class="form-control" name="title_text"></textarea>
    </div>
    <div class="form-group">
        <label>Post</label>
        <textarea class="form-control" name="publish_text" rows="5"></textarea>
    </div>

<button type="submit" class="btn btn-primary" name="button">Publish</button>
</form>
<%- include("partials/footer"); -%>